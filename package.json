{
  "name": "sipce-api",
  "version": "1.2.0",
  "description": "",
  "author": "Verónica Reyes, Esmeralda López, Fernando Rivas, Kevin Rodríguez",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"migrations/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:gen": "nest build && npm run typeorm -- migration:generate -n",
    "migration:create": "npm run typeorm -- migration:create -n",
    "migration:run": "nest build && typeorm migration:run",
    "migration:revert": "nest build && typeorm migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run format && npm run lint && npm test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run format && npm run lint"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/bull": "^0.2.3",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.4.4",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^7.4.4",
    "@types/faker": "^5.1.5",
    "bcrypt": "^4.0.1",
    "bull": "^3.18.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cloudinary": "^1.22.0",
    "express-rate-limit": "^5.1.3",
    "generate-password": "^1.5.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.13",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "pug": "^3.0.0",
    "puppeteer": "^5.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rrule": "^2.6.6",
    "rxjs": "^6.5.4",
    "socket.io-redis": "^5.4.0",
    "streamifier": "^0.1.1",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1",
    "typeorm-transactional-cls-hooked": "^0.1.12"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.14.4",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^17.1.4",
    "@types/helmet": "0.0.46",
    "@types/jest": "25.1.4",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.161",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.9.1",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/puppeteer": "^5.4.0",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-redis": "^1.0.26",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=12.18"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^@auth(.*)$": "<rootDir>/auth$1",
      "^@users(.*)$": "<rootDir>/users$1",
      "^@core(.*)$": "<rootDir>/core$1",
      "^@mails(.*)$": "<rootDir>/mails$1",
      "^@logs(.*)$": "<rootDir>/logs$1",
      "^@academics(.*)$": "<rootDir>/academics$1",
      "^@students(.*)$": "<rootDir>/students$1",
      "^@schedules(.*)$": "<rootDir>/schedules$1",
      "^@counseling(.*)$": "<rootDir>/counseling$1",
      "^@fouls(.*)$": "<rootDir>/fouls$1",
      "^@sanctions(.*)$": "<rootDir>/sanctions$1",
      "^@reporting(.*)$": "<rootDir>/reporting$1",
      "^@expedient(.*)$": "<rootDir>/expedient$1",
      "^@sociometrics(.*)$": "<rootDir>/sociometrics$1",
      "^@history(.*)$": "<rootDir>/history$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
